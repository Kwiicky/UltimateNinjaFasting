<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acdc.ultimateninjafasting">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ProgrammationActivity" />
        <activity android:name=".ReglageProgrammationActivity" />
        <activity android:name=".ProgrammationDetailsActivity"></activity>


        <provider
            android:name="provider.MyContentProvider"
            android:authorities="ultimate.ninja.fasting"
            android:enabled="true"
            android:exported="true"></provider>

    </application>

</manifest>